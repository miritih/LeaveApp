 function actionsStyler(value,row,index){
        var now=new Date();
        var y = now.getFullYear();
        var m = now.getMonth()+1;
        var d = now.getDate();
        var z= y+'-'+(m<10?('0'+m):m)+'-'+(d<10?('0'+d):d);
        if (row.date <= z){
            return 'background-color:rgba(21, 21, 21, 0.04);color:#000;opacity:0.4;font-weight:bold;';
        } else {
            return 'background-color:rgba(21, 21, 21, 0.04);color:#000;opacity:0.4;font-weight:bold;';
        }
    }